!URxvt.font: xft:Inconsolata\-g for Powerline:pixelsize=36
!URxvt.font: xft:Liberation Mono for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG L DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG L for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG M DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG M for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG S DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG S for Powerline:pixelsize=36
!URxvt.font: xft:Source Code Pro for Powerline:pixelsize=36
!URxvt.font: xft:Terminess Powerline:pixelsize=36:style=bold
!URxvt.font: xft:Ubuntu Mono derivative Powerline:pixelsize=36
URxvt.internalBorder: 0
URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal


