URxvt.buffered: true
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,font-size,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt*termName: rxvt-unicode-256color
URxvt.transparent: true
URxvt*shading: 30
URxvt.background: #080808
URxvt.scrollBar: false
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.dpi: 96
Xft.rgba: rgb
!URxvt.font: xft:Anonymous Pro for Powerline:pixelsize=36
!URxvt.font: xft:Bitstream Vera Sans Mono for Powerline:pixelsize=36
!URxvt.font: xft:DejaVu Sans Mono for Powerline:pixelsize=36
!URxvt.font: xft:Droid Sans Mono for Powerline:pixelsize=36
URxvt.font: xft:Inconsolata for Powerline:pixelsize=36
